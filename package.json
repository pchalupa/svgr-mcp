{
  "name": "svgr-mcp",
  "version": "1.2.4",
  "description": "MCP server for converting SVG to React components using SVGR",
  "keywords": [
    "mcp",
    "svgr",
    "svg",
    "react",
    "converter"
  ],
  "author": "pchalupa <chalupa.petr93@gmail.com> (https://github.com/pchalupa)",
  "homepage": "https://github.com/pchalupa/svgr-mcp#readme",
  "bugs": {
    "url": "https://github.com/pchalupa/svgr-mcp/issues"
  },
  "repository": {
    "url": "https://github.com/pchalupa/svgr-mcp"
  },
  "license": "MIT",
  "type": "module",
  "main": "dist/index.cjs",
  "bin": {
    "svgr-mcp": "./dist/index.cjs"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup",
    "inspect": "pnpm dlx @modelcontextprotocol/inspector node dist/index.cjs",
    "lint": "biome lint",
    "lint:fix": "biome check --write",
    "format": "biome format --write",
    "format:check": "biome format",
    "ts:check": "tsc --noEmit",
    "prepublishOnly": "tsup",
    "release": "release-it"
  },
  "dependencies": {
    "@modelcontextprotocol/sdk": "1.25.1",
    "@svgr/core": "8.1.0",
    "@svgr/plugin-jsx": "8.1.0",
    "@svgr/plugin-prettier": "8.1.0",
    "@svgr/plugin-svgo": "8.1.0",
    "zod": "~4.3.5"
  },
  "devDependencies": {
    "@biomejs/biome": "^2.3.11",
    "@types/node": "^25.0.3",
    "release-it": "^19.2.2",
    "tsup": "^8.5.1",
    "typescript": "^5.9.3"
  },
  "packageManager": "pnpm@10.27.0"
}
